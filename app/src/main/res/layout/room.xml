            app:layout_constraintEnd_toStartOf="@+id/editText2"
            app:layout_constraintHorizontal_bias="0.328"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText2" />

        <CheckBox
            android:id="@+id/ze"
            android:layout_width="70dp"
            android:layout_height="25dp"
            android:layout_alignBaseline="@+id/wa2"
            android:layout_alignBottom="@+id/wa2"
            android:layout_alignLeft="@+id/fa"
            android:layout_alignStart="@+id/fa"
            android:text="Zero"
            app:layout_constraintBottom_toTopOf="@+id/checkBox7"
            app:layout_constraintEnd_toStartOf="@+id/checkBox2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/checkBox5" />

        <CheckBox
            android:id="@+id/li"
            android:layout_width="70dp"
            android:layout_height="30dp"

            android:layout_alignBaseline="@+id/co2"
            android:layout_alignBottom="@+id/co2"
            android:layout_alignLeft="@+id/so"
            android:layout_alignStart="@+id/so"
